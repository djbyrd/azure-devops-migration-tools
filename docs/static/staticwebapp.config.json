{
  "routes": [
    {
      "route": "/version.json",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type"
      }
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/404"
    }
  }
}
